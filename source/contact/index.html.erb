---
title: Contact
---

<div class="content">
  <div class="contact-container">
    <div class="contact-form contact-form-width">
      <div class="head-contact-img" style="background-image: linear-gradient(0deg, rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('/images/projets/guinee_equatoriale_oyala_tribune_presidentielle/guinee_equatoriale_oyala_tribune_presidentielle_livraison01.jpg');">
        <div class="postacard">
          <img src="/images/logo/Logo_blanc_5.png" alt="" style="
          max-width: 60%">
        </div>
      </div>
      <form action="https://formspree.io/contact@leopard-architecture.fr" method="POST" >
        <div class="contact-form-content">
          <div class="input-group margin-form">
            <div class="input-group-prepend">
              <span class="input-group-text font-size-form">Prénom et nom</span>
            </div>
            <input type="text" aria-label="First name" class="form-control font-size-form" placeholder="Prénom" name="Prénom">
            <input type="text" aria-label="Last name" class="form-control font-size-form" placeholder="Nom" name="Nom">
          </div>

          <div class="input-group margin-form">
            <div class="input-group-prepend">
              <span class="input-group-text font-size-form" id="basic-addon1">Email </span>
            </div>
            <input type="text" class="form-control font-size-form" placeholder="exemple@email.com" aria-label="exemple@email.com" aria-describedby="basic-addon1" name="Mail de contact">
          </div>

          <div class="input-group margin-form">
            <div class="input-group-prepend">
              <span class="input-group-text font-size-form">Votre message</span>
            </div>
            <textarea class="form-control font-size-form" aria-label="With textarea" placeholder="Je vous écoute ..." name="Message"></textarea>
          </div>
          <input class="margin-form contact-btn font-size-form" type="submit" value="Envoyer">
        </div>
      </form>
    </div>
  </div>
</div>
